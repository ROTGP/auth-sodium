{
    "name": "rotgp/auth-sodium",
    "description": "Laravel authentication with ED25519 signatures",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ROTGP\\AuthSodium\\": "src"
        },
        "classmap": [
            "src"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ROTGP\\AuthSodium\\Test\\": "tests/"
        },
        "classmap": [
            "tests/models",
            "tests/controllers"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "orchestra/testbench": "^6.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ROTGP\\AuthSodium\\AuthSodiumServiceProvider"
            ],
            "aliases": {
                "AuthSodium": "ROTGP\\AuthSodium\\Facades\\AuthSodium"
            }
        }
    }
}